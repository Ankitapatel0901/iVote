<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>loginServlet</servlet-name>
  	<display-name>loginServlet</display-name>
  	<description></description>
  	<servlet-class>com.login.loginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>InsertCandidate</servlet-name>
  	<display-name>InsertCandidate</display-name>
  	<description></description>
  	<servlet-class>com.controller.InsertCandidate</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>deleteCandidate</servlet-name>
  	<display-name>deleteCandidate</display-name>
  	<description></description>
  	<servlet-class>com.controller.deleteCandidate</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ShowCandidate</servlet-name>
  	<display-name>ShowCandidate</display-name>

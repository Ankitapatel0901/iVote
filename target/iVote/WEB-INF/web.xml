  	<servlet-name>ShowCandidates</servlet-name>
  	<display-name>ShowCandidates</display-name>
  	<description></description>
  	<servlet-class>com.login.ShowCandidates</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>deleteCandidates</servlet-name>
  	<display-name>deleteCandidates</display-name>
  	<description></description>
  	<servlet-class>com.login.deleteCandidates</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>InsertCandidates</servlet-name>
  	<display-name>InsertCandidates</display-name>
  	<description></description>
  	<servlet-class>com.login.InsertCandidates</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>loginServlet</servlet-name>
  	<url-pattern>/loginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>InsertCandidate</servlet-name>
  	<url-pattern>/InsertCandidate</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>deleteCandidate</servlet-name>
  	<url-pattern>/deleteCandidate</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ShowCandidate</servlet-name>
  	<url-pattern>/ShowCandidate</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ShowCandidates</servlet-name>
  	<url-pattern>/ShowCandidates</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>deleteCandidates</servlet-name>
  	<url-pattern>/deleteCandidates</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>InsertCandidates</servlet-name>
  	<url-pattern>/InsertCandidates</url-pattern>
  </servlet-mapping>
</web-app>
